<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.COVID19" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FFFF</item>
        <item name="colorPrimaryVariant">#FFFF</item>
        <item name="colorOnPrimary">#FFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFFF</item>
        <item name="colorSecondaryVariant">#FFFF</item>
        <item name="colorOnSecondary">#FFFF</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.COVID19.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.COVID19.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.COVID19.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>